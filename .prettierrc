{
  "singleQuote": true,
  "tabWidth": 2,
  "printWidth": 80,
  "endOfLine": "auto",
  "useTabs": false,
  "semi": true,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid",
  "htmlWhitespaceSensitivity": "css",
  "singleAttributePerLine": false
}
